<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	                    http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
<!-- coding DAO's -->


	<bean name="rolePermissionService" class="com.claim.model.service.impl.RolePermissionServiceImpl">
		<property name="dao" ref="rolePermissionDao"></property>
	</bean>

	<bean name="sysPermissionService" class="com.claim.model.service.impl.SysPermissionServiceImpl">
		<property name="dao" ref="sysPermissionDao"></property>
	</bean>

	<bean name="sysRoleService" class="com.claim.model.service.impl.SysRoleServiceImpl">
		<property name="dao" ref="sysRoleDao"></property>
	</bean>
	
	<bean name="workflowDefinitionService" class="com.claim.model.service.impl.WorkflowDefinitionServiceImpl">
		<property name="dao" ref="workflowDefinitionDao"></property>
	</bean>
	
	<bean name="sysUserService" class="com.claim.model.service.impl.SysUserServiceImpl">
		<property name="dao" ref="sysUserDao"></property>
	</bean>

	<bean name="userRoleService" class="com.claim.model.service.impl.UserRoleServiceImpl">
		<property name="dao" ref="userRoleDao"></property>
	</bean>
	
		
	<bean name="claimService" class="com.claim.model.service.impl.ClaimServiceImpl">
		<property name="dao" ref="claimDao"></property>
	</bean>
	
	<bean name="claimTypeService" class="com.claim.model.service.impl.ClaimTypeServiceImpl">
		<property name="dao" ref="claimTypeDao"></property>
	</bean>
	
	<bean name="claimWorkflowStepService" class="com.claim.model.service.impl.ClaimWorkflowStepServiceImpl">
		<property name="dao" ref="claimWorkflowStepDao"></property>
	</bean>
	
	<bean name="claimWorkflowService" class="com.claim.model.service.impl.ClaimWorkflowServiceImpl">
		<property name="dao" ref="claimWorkflowDao"></property>
	</bean>
	
	<bean name="claimStatusService" class="com.claim.model.service.impl.ClaimStatusServiceImpl">
		<property name="dao" ref="claimStatusDao"></property>
	</bean>
	
	<bean name="claimDocumentService" class="com.claim.model.service.impl.ClaimDocumentServiceImpl">
		<property name="dao" ref="claimDocumentDao"></property>
	</bean>
	
	<bean name="claimStatusStepService" class="com.claim.model.service.impl.ClaimStatusStepServiceImpl">
		<property name="dao" ref="claimStatusStepDao"></property>
	</bean>
</beans>
