		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean name="claimStatusDao" class="com.claim.model.dao.impl.ClaimStatusDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean name="claimDocumentDao" class="com.claim.model.dao.impl.ClaimDocumentDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean name="claimStatusStepDao" class="com.claim.model.dao.impl.ClaimStatusStepDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
</beans>
