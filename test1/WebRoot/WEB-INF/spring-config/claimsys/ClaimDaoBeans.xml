<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	                    http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
<!-- coding DAO's -->


	<bean name="rolePermissionDao" class="com.claim.model.dao.impl.RolePermissionDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>

	<bean name="sysPermissionDao" class="com.claim.model.dao.impl.SysPermissionDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>

	<bean name="sysRoleDao" class="com.claim.model.dao.impl.SysRoleDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>

	<bean name="sysUserDao" class="com.claim.model.dao.impl.SysUserDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>

	<bean name="userRoleDao" class="com.claim.model.dao.impl.UserRoleDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
</beans>
